###  https://w3id.org/GConsent#hasConsentGivenByDelegate
:hasConsentGivenByDelegate <http://purl.org/vocab/vann/example> """
@prefix gconsent: <https://w3id.org/GConsent#> .
@prefix : <http://example.com/> .

:consent_hjp_1808 a gconsent:ConsentInstance ;
    rdfs:label \"Emergency consent for Mr. Harshvardhan\" ;
    rdfs:comment \"Harshvardhan had an accident and was brought to the hospital. During this time, his vitals and other information were taken for hospital records.\" ;
    gconsent:isConsentInstanceOf :Consent_hjp ;
    gconsent:wasGivenBy :delegatedconsent ;
    gconsent:wasGivenAtInstant \"2099-12-31-00-00-00\" ;
    gconsent:wasGivenAtLocation \"Hospital\" ;
    gconsent:changesConsentStatusTo gconsent:ConsentStatusExplicitlyGiven .

:delegatedconsent a gconsent:Delegation ;
    rdfs:label \"Delegated consent for Mr. Harshvardhan by Hospital Staff\" ;
    gconsent:hasConsentGivenByDelegate \"Hospitanl Staff Member 4826719\" ;  # <----
    gconsent:isDelegationForDataSubject :HJP ."""^^xsd:string ;
                           rdfs:comment "Specifies the _delegate_ that gave consent. The range of this property can be a `DataSubject` or any entity, including cases where an entity is as an automated mechanism."^^xsd:string ;
                           rdfs:label "hasConsentGivenByDelegate"^^xsd:string ;
                           rdf:type owl:AnnotationProperty ;
                           rdfs:domain :ConsentDelegation .


###  https://w3id.org/GConsent#hasContext
:hasContext <http://purl.org/vocab/vann/example> """
@prefix gconsent: <https://w3id.org/GConsent#> .
@prefix : <http://example.com/> .

:capturedThroughWebForm rdf:type rdf:Property, owl:ObjectProperty ;
    rdfs:label \"capturedThroughWebForm\" ;
    rdfs:subPropertyOf gconsent:hasContext ;  # <----
    owl:subPropertyOf gconsent:hasContext ;  # <----
    rdfs:comment \"Links the captured/obtained consent with the specific version of web form filled by the user.\" ;
    rdfs:domain gconsent:ConsentInstance ;
    rdfs:range :WebForm .
:isWebFormForGivenConsent rdf:type rdf:Property, owl:ObjectProperty ;
    rdfs:label \"isWebFormForGivenConsent\" ;
    rdfs:subPropertyOf :isContextForConsentInstance ;
    owl:subPropertyOf :isContextForConsentInstance ;
    rdfs:comment \"Links the Web Form object to the consent instances obtained through it.\" ;
    rdfs:domain :WebForm ;
    rdfs:range gconsent:ConsentInstance ."""^^xsd:string ;
            rdfs:comment "The parent property linking all `ConsentInstance` to contexts."^^xsd:string ;
            rdfs:label "hasContext"^^xsd:string ;
            rdf:type owl:AnnotationProperty ;
            rdfs:domain :ConsentInstance .


###  https://w3id.org/GConsent#hasExpiry
:hasExpiry <http://purl.org/vocab/vann/example> """
@prefix gconsent: <https://w3id.org/GConsent#> .
@prefix : <http://example.com/> .

:HJP a gconsent:DataSubject ;
    foaf:name \"Harshvardhan\" .

:consent_hjp_1808 a gconsent:ConsentInstance ;
    rdfs:label \"Email op-out\" ;
    gconsent:isConsentInstanceOf :Consent_hjp ;
    gconsent:wasGivenBy :HJP ;
    gconsent:changesConsentStatusTo gconsent:ConsentStatusWithdrawn ;
    gconsent:wasGivenAtInstant \"2099-12-31-00-00-00\" ;
    gconsent:hasExpiry \"2 years\" .  # <----

:Consent_hjp a gconsent:Consent ;
    gconsent:isConsentFor :HJP ;
    gconsent:hasConsentInstance :consent_hjp_1808 ;
    gconsent:hasStatus gconsent:ConsentStatusWithdrawn ."""^^xsd:string ;
           rdfs:comment "Specifies the expiry of the (current) current instance. The value of this expiry can be an instant in time, such as a specific date or datetime, or can be a duration."^^xsd:string ;
           rdfs:label "hasExpiry"^^xsd:string ;
           rdf:type owl:AnnotationProperty ;
           rdfs:domain :ConsentInstance .


